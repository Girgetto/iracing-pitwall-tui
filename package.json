{
  "name": "iracing-live-telemetry",
  "version": "0.1.1",
  "description": "Live iRacing telemetry timing table in the terminal",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "start": "node index.js",
    "build": "npx @yao-pkg/pkg . --targets node24-win-x64 --output dist/iracing-pitwall-tui.exe"
  },
  "pkg": {
    "assets": [
      "node_modules/iracing-sdk-js/build/Release/IrSdkNodeBindings.node"
    ],
    "scripts": [
      "index.js"
    ]
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.5",
    "iracing-sdk-js": "^1.5.0"
  },
  "devDependencies": {
    "@yao-pkg/pkg": "^6.14.1",
    "pkg": "^5.8.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
